@page "/AccesoDenedado"
@model AccessDeniedModel
@{
    ViewData["Title"] = "Acceso denegado";
}
<div class="container">
    <div class="row">
        <div class="col-6 d-flex align-items-center">
            <div>
                <h1 class="text-danger">@ViewData["Title"]</h1>
                <h6 class="text-danger">No tiene permisos para acceder a este recurso</h6>
            </div>
        </div>
        <div class="col-6 align-items-center">
            <img src="~/img/illustrations/acceso_denegado.svg" alt="imagen de acceso denegado" class="imagen-width img-fluid" />
        </div>
    </div>
</div>
